    "st=str(n)\n",
    "le=len(st)\n",
    "armstrong=0\n",
    "for i in st:\n",
    "    first = int(i)\n",
    "    armstrong+= first**le\n",
    "    if armstrong == n:\n",
    "        print(n,\"armstrong number\")\n",
    "    else:\n",
    "        print(\"not a armstrong number\")\n"
   ]
